{
  "mcpServers": {
    "voicevox-go": {
      "command": "mcp-voicevox-go",
      "args": [
        "stdio"
      ],
      "env": {
        "MCP_VOICEVOX_ENABLE_PLAYBACK": "true",
        "MCP_VOICEVOX_DEFAULT_SPEED_SCALE": "1.5"
      },
      "autoApprove": [
        "get_speakers",
        "text_to_speech"
      ],
      "timeout": 3000000
    }
  }
}