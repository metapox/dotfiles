{
  "mcpServers": {
    "voicevox-go": {
      "command": "mcp-voicevox",
      "args": [
        "stdio"
      ],
      "env": {
        "MCP_VOICEVOX_ENABLE_PLAYBACK": "true"
      },
      "autoApprove": [
        "get_speakers",
        "text_to_speech"
      ],
      "timeout": 3000000
    }
  }
}